{
  "name": "rosette-app",
  "private": true,
  "description": "A front end for the Rosette protocol",
  "version": "0.0.1",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "yarn sync-assets && cross-env NODE_ENV=production remix build",
    "dev": "cross-env NODE_ENV=development remix dev",
    "start": "cross-env NODE_ENV=production remix-serve build",
    "sync-assets": "copy-aragon-ui-assets ./public",
    "postinstall": "npx patch-package && remix setup node",
    "lint": "eslint ./app"
  },
  "dependencies": {
    "@1hive/1hive-ui": "^1.0.18",
    "@remix-run/react": "^1.2.3",
    "@remix-run/vercel": "^1.2.3",
    "@udecode/zustood": "^0.4.4",
    "buffer": "^6.0.3",
    "clipboard-polyfill": "^4.0.0-rc1",
    "ethers": "^5.6.1",
    "ipfs-http-client": "^55.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-spring": "^9.4.4",
    "remix": "^1.2.3",
    "styled-components": "^5.3.3",
    "wagmi": "^0.2.21",
    "zustand": "^3.7.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.2.3",
    "@remix-run/eslint-config": "^1.2.3",
    "@remix-run/serve": "^1.2.3",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.24",
    "@types/stylis": "^4.0.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.9.0",
    "patch-package": "^6.4.7",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=14"
  }
}
